{
    "name": "tombroucke/otomaties-sage-helper",
    "description": "This package provides an easy way to reuse files across projects",
    "authors": [
        {
            "name": "Tom Broucke",
            "email": "tom@tombroucke.be"
        }
    ],
    "require": {
      "roots/acorn": "*"
    },
    "autoload": {
      "psr-4": {
        "Otomaties\\Sage\\": "src/"
      }
    },
    "extra": {
      "acorn": {
        "providers": [
          "Otomaties\\Sage\\OtomatiesServiceProvider"
        ]
      }
    }
}
